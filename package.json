{
  "version": "0.0.26",
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    ".": "dist/index.ts",
    "./types": "dist/types.ts",
    "./constants": "dist/constants.ts",
    "./utils": "dist/utils.index.ts"
  },
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "yarn build:dev --watch",
    "build:dev": "rimraf dist && tsup src/index.ts src/constants.ts src/types.ts src/utils/index.ts --dts --sourcemap",
    "build": "yarn build:dev --minify",
    "test": "jest",
    "release": "yarn build  && yarn test && npm publish --access public",
    "v:patch": "npm version patch",
    "v:minor": "npm version minor",
    "v:major": "npm verion major",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "docs": "typedoc"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ]
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "constants.ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "name": "@apwine/sdk",
  "author": "APWine",
  "description": "SDK for the APWine Protocol",
  "module": "dist/apwine-sdk.esm.js",
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@size-limit/preset-small-lib": "^7.0.4",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.2",
    "@types/ramda": "^0.27.60",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "graphology-types": "^0.23.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "rimraf": "^3.0.2",
    "size-limit": "^7.0.4",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "tsup": "^5.11.11",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@0xsequence/multicall": "^0.35.2",
    "@apwine/amm": "^0.0.9",
    "@apwine/protocol": "^0.0.10",
    "@ethersproject/logger": "^5.5.0",
    "@ethersproject/providers": "^5.5.1",
    "@uniswap/sdk": "^3.0.3",
    "ethers": "^5.5.2",
    "graphology": "^0.23.2",
    "graphology-shortest-path": "^2.0.0",
    "ramda": "^0.27.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
